{
  "name": "@geobase-js/geoai",
  "version": "0.0.5",
  "description": "A JavaScript library for running Geo AI models in frontend applications",
  "keywords": [
    "geoai",
    "geospatial",
    "ai",
    "machine-learning",
    "satellite-imagery",
    "object-detection",
    "segmentation",
    "javascript",
    "typescript"
  ],
  "author": "Decision Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decision-labs/geobase-ai.js.git"
  },
  "homepage": "https://docs.geobase.app/geoaijs",
  "bugs": "https://github.com/decision-labs/geobase-ai.js/issues",
  "files": [
    "@geobase-js",
    "assets",
    "*.d.ts",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "main": "./@geobase-js/geoai.js",
  "module": "./@geobase-js/geoai.js",
  "unpkg": "./@geobase-js/geoai.js",
  "jsdelivr": "./@geobase-js/geoai.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./@geobase-js/geoai.js",
      "types": "./index.d.ts"
    },
    "./dist": {
      "import": "./@geobase-js/geoai.js"
    }
  },
  "types": "./index.d.ts",
  "peerDependencies": {
    "@huggingface/transformers": "^3.3.3",
    "@techstark/opencv-js": "^4.10.0-release.1",
    "onnxruntime-web": "^1.21.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf build/**/* && pnpm build:core && pnpm build:types && pnpm build:package",
    "prepublish:prep": "node ./scripts/prepare-publish.mjs",
    "publish:build": "cd build && pnpm publish --access public --ignore-scripts",
    "publish:pkg": "pnpm prepublish:prep && pnpm publish:build",
    "build:core": "tsc && vite build",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:package": "copyfiles ./package.json build",
    "build:examples": "cp -r ./build ./examples/codepen-geobase",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:build": "vitest run test/build.test.ts",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "pnpm format:scripts && pnpm format:styles",
    "prepare": "husky && echo 'npx lint-staged' > .husky/pre-commit && git add .husky/pre-commit",
    "uninstall-husky": "pnpm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "terser": "^5.39.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-plugin-commonjs": "^0.10.4",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@turf/bbox": "^7.2.0",
    "@turf/bbox-polygon": "^7.2.0",
    "global-mercator": "github:decision-labs/global-mercator",
    "node-fetch": "^3.3.2"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}
